<link rel="stylesheet" type="text/css" href="~/Content/CustomCss/Index/leftPartialViews/leftTopPanel.css" />
<link rel="stylesheet" type="text/css" href="~/Content/CustomCss/Index/GridView/GridCss.css" />



    

        <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Add a Recipe:</h5>

                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        <div id="modalAlert" class="alert alert-success hide" role="alert">
                            Recipe Saved: Lets freaking go!
                        </div>
                    </div>



                    <div class="modal-preview-container ">
                        @*Display what model would look like*@
                        <div class="modal-preview col-xs-12">



                            <div class="grid-item grid-preview-item">
                                <div class="card" style="width: 100%;">
                                    <div class="grid-image-container">
                                        <img id="previewImage" class="card-img-top" src="data:image/jpeg;base64," alt="Recipe Image Preview">

                                    </div>
                                    <div class="card-body">
                                        <div class="grid-card-title">
                                            <h5 class="card-title" id="card-title-preview">Title</h5>
                                        </div>
                                        <div class="recipeDescContainer">
                                            <p id="previewDescription" class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                                        </div>
                                        <a href="#" class="btn btn-primary grid-button">Take a look</a>
                                    </div>
                                </div>
                            </div>



                        </div>



                        <div class="modal-body col-xs-12">
                            <form id="add-recipe-modal-form" @*action="Home/AddRecipe"*@ type="post">
                                <div class="col-md-12 col-xs-12">
                                    <Label id="recipeLabel" class="form-control">Recipe Title</Label>
                                    <input value="" maxlength="40" id="modal-input-title" type="text" name="title" class="form-control" placeholder="..." />
                                </div>

                                <div class="col-md-12 col-xs-12">
                                    <Label id="recipeLabel" class="form-control">Recipe Description</Label>
                                    <input id="modal-description-preview" type="text" name="description" class="form-control" placeholder="..." maxlength="40" />
                                </div>

                                <div class="col-md-12 col-xs-12">
                                    <Label id="recipeLabel" class="form-control">Ingredients</Label>

                                    
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                    <input class="form-control" type="text" name="ingredients" placeholder="..." />
                                </div>

                                <div class="col-md-12 col-xs-12">
                                    <Label id="recipeLabel" class="form-control">Instructions</Label>

                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                    <input class="form-control" type="text" name="instructions" placeholder="...">
                                </div>

                                <div class="col-md-6 col-xs-12">
                                    <Label id="recipeLabel" class="form-control">Prep Time (Minutes)</Label>
                                    <input id="modal-input" type="number" name="prepTime" class="form-control" placeholder="..." />

                                </div>

                                <div class="col-md-3 col-xs-12">
                                    <Label id="recipeLabel" class="form-control">Servings</Label>

                                    <input id="modal-input" type="number" name="servings" class="form-control" placeholder="..." />
                                </div>

                                <div class="col-md-3 col-xs-12">
                                    <Label id="recipeLabel" class="form-control">Calories</Label>

                                    <input id="modal-input" type="number" name="calories" class="form-control" placeholder="..." />
                                </div>

                                <div class="col-md-12 col-xs-12">
                                    <Label id="recipeLabel" class="form-control">Image URL</Label>
                                    <input id="modal-preview-image" type="url" name="imageURL" class="form-control" placeholder="..." />
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal" id="Modalclose">Close</button>
                                    <button type="submit" class="btn btn-primary" id="modalSubmitButton">Save changes</button>
                                </div>

                            </form>
                        </div>
                    </div>

                </div>
            </div>
        </div>

    <script src="~/Scripts/Add Recipe Modal.js"></script>
